{
	"name": "vscode-js",
	"icon": "images/logo.png",
	"displayName": "js",
	"description": "A VS Code extension for quick access to js, supporting multi-language selection and simultaneous viewing in both VS Code and the system browser.",
	"version": "0.0.1",
	"license": "MIT",
	"publisher": "zhang-renyang",
	"keywords": [
		"js",
		"Official Documentation",
		"Community",
		"Tutorials",
		"AI Code Completion",
		"Real-time Collaboration",
		"Integrated Terminal",
		"CLI",
		"Debugging Tools",
		"Version Control",
		"Plugins"
	],
	"repository": {
		"url": "https://github.com/zhangrenyang/vscode-js"
	},
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "js.switchLanguage",
				"title": "js: Switch Language"
			},
			{
				"command": "js.openLinkInBrowser",
				"title": "Open Link in Browser"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "jsViewContainer",
					"title": "js",
					"icon": "images/logo.svg"
				}
			]
		},
		"views": {
			"jsViewContainer": [
				{
					"id": "jsView",
					"name": "js"
				}
			]
		},
		"menus": {
			"view/item/context": [
				{
					"command": "js.openLinkInBrowser",
					"when": "viewItem == jsView && viewItem.contextValue == linkItem",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/open": "^6.1.0",
		"@types/vscode": "^1.75.0",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"@vscode/test-electron": "^2.2.2",
		"copy-webpack-plugin": "^12.0.2",
		"eslint": "^8.33.0",
		"glob": "^8.1.0",
		"mocha": "^10.1.0",
		"tailwindcss": "^3.4.12",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"dependencies": {
		"open": "^10.1.0"
	},
	"files": [
		"dist/**/*",
		"src/**/*",
		"images/**/*"
	],
